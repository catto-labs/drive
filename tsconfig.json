{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Node",
    "jsxImportSource": "solid-js",
    "jsx": "preserve",
    "strict": true,
    "types": ["solid-start/env", "unplugin-icons/types/solid"],
    "baseUrl": "./",
    "paths": {
      "@/*": ["./src/*"]
    }
  }
}
